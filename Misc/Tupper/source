import os
import zipfile

#生成
file_content = ''
l= len(file_content) 

os.makedirs("challenge",exist_ok=True)
for i in range(l):
    zip_name = str(i)+".zip"
    with zipfile.ZipFile(f"challenge/{zip_name}","w",zipfile.ZIP_DEFLATED) as zip_file:
        zip_file.setpassword = ("123456")
        file_names = str(i)+".txt"
        file_content_list = file_content[i]
        
        zip_file.writestr(f"{file_names}",file_content_list)
